        <mxCell id="i-nkXKOmJGmnzbzkcPUM-8" value="&lt;span style=&quot;text-align: left;&quot;&gt;signal_address&amp;nbsp;待细看&lt;/span&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
